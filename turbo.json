{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".expo/**",
                "dist/**",
                ".turbo/**"
            ],
            "env": [
                "EXPO_PUBLIC_*"
            ]
        },
        "start": {
            "cache": false,
            "persistent": true
        },
        "android": {
            "cache": false,
            "persistent": true
        },
        "ios": {
            "cache": false,
            "persistent": true
        },
        "web": {
            "cache": false,
            "persistent": true
        },
        "lint": {
            "outputs": []
        },
        "type-check": {
            "dependsOn": [
                "^build"
            ],
            "outputs": []
        },
        "clean": {
            "cache": false
        }
    },
    "globalEnv": [
        "NODE_ENV",
        "EXPO_PUBLIC_*"
    ],
    "globalDependencies": [
        "package.json",
        "pnpm-lock.yaml",
        "tsconfig.json",
        "babel.config.js",
        "tailwind.config.js"
    ]
}